<+>@import url('https://fonts.googleapis.com/css2?family=Montserrat:wght@600&family=Tenor+Sans&family=Work+Sans:wght@300&display=swap');\r\n\r\nheader .logo-text {\r\n    font-family: 'Montserrat', sans-serif;\r\n    font-weight: 600;\r\n    font-size: 25px;\r\n    height: inherit;\r\n    vertical-align: middle;\r\n}\r\n\r\nheader .presentation {\r\n    margin: 50px 0;\r\n}\r\n\r\nimg, .logo-text {\r\n    display: inline-block;\r\n    vertical-align: middle;\r\n}\r\n\r\n.main-block {\r\n    display: flex;\r\n    align-items: flex-start;\r\n    position: relative;\r\n}\r\n\r\n@media (max-width: 1230px) {\r\n    .main-img:not(:last-child) {\r\n        margin-right: calc(15px + 35 * ((100vw - 320px) / 910));\r\n    }\r\n}\r\n\r\n@media (max-width: 1920.2px) {\r\n    .main-img {\r\n        flex-basis: 720px;\r\n    }\r\n}\r\n\r\n@media (max-width: 1920.2px) and (max-width: 1230px) {\r\n    .main-img {\r\n        flex-basis: calc(120px + 600 * ((100vw - 320px) / 910));\r\n    }\r\n}\r\n\r\n.main-text {\r\n    display: flex;\r\n    flex-direction: column;\r\n}\r\n\r\nh1 {\r\n    font-family: \"Tenor Sans\";\r\n    font-size: 36px;\r\n    font-weight: 500;\r\n}\r\n\r\nh5 {\r\n    text-transform: uppercase;\r\n    font-size: 10px;\r\n    font-weight: 400;\r\n    line-height: 120%;\r\n    letter-spacing: 1px;\r\n    color: #999999;\r\n}\r\n\r\n.banner {\r\n    vertical-align: top;\r\n}\r\n\r\n/*----------------------------------NAV-------------------------------------------------------------------------------*/\r\nheader ul.nav {\r\n    float: right;\r\n    list-style: none;\r\n}\r\n\r\nheader ul.nav li {\r\n    display: inline;\r\n    margin-left: 25px;\r\n    cursor: pointer;\r\n    font-weight: 400;\r\n}\r\n\r\n/*----------------------------------MEDIA-RULES-----------------------------------------------------------------------*/\r\n
